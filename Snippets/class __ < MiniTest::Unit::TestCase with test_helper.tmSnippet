	<string>require File.dirname(__FILE__) + "/test_helper"

require "${1:`#!/usr/bin/env ruby
    require 'rubygems'
    require "active_support"
    puts (ENV['TM_FILENAME'] || 'test_some_model.rb').gsub(/\.rb$/, '').gsub(/^test_/,'').gsub(/_test$/,'')
