class Test${2:${1/([\w&amp;&amp;[^_]]+)|./\u$1/g}} &lt; MiniTest::Unit::TestCase
	def test_${3:case_name}
		$0
	end
end</string>
	<key>name</key>
	<string>class .. &lt; MiniTest::Unit::TestCase with test_helper</string>
	<key>scope</key>
